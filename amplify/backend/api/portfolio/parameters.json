{
  "api": {
    "AppSyncApiName": "portfolio",
    "output": {
      "authConfig": {
        "defaultAuthentication": {
          "apiKeyConfig": {
            "apiKeyExpirationEpoch": 604800
          },
          "authenticationType": "API_KEY"
        }
      }
    }
  },
  "DynamoDBBillingMode": "PAY_PER_REQUEST",
  "DynamoDBEnableServerSideEncryption": false
}